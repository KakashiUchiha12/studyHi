{
    "name": "studyhi",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "next build --webpack",
        "dev": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" server.ts",
        "start": "next start",
        "build:server": "tsc --project tsconfig.server.json || echo 'Server build emitted with warnings'",
        "start:cpanel": "NODE_ENV=production node dist/server.js",
        "prepare:cpanel": "npm run build && npm run build:server",
        "lint": "next lint",
        "start:production": "set NODE_ENV=production && next start",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:ci": "jest --ci --coverage --watchAll=false",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:debug": "playwright test --debug",
        "test:e2e:install": "playwright install",
        "test:runner": "node scripts/test-runner.js",
        "test:performance": "node scripts/performance-test.js",
        "test:accessibility": "node scripts/accessibility-test.js",
        "test:coverage-report": "node scripts/coverage-report.js",
        "test:all": "npm run test:ci && npm run test:e2e",
        "test:quick": "jest --passWithNoTests --silent",
        "test:debug": "jest --detectOpenHandles --forceExit",
        "build:analyze": "ANALYZE=true npm run build",
        "build:production": "npm run build",
        "lint:fix": "next lint --fix",
        "type-check": "tsc --noEmit",
        "clean": "node -e \"const fs = require('fs'); ['out', '.next', 'coverage', 'test-results', 'playwright-report'].forEach(dir => fs.rmSync(dir, { recursive: true, force: true }));\"",
        "prebuild": "npm run clean",
        "postbuild": "echo 'Build completed successfully'",
        "production:build": "node scripts/production-build.js",
        "deploy:production": "node scripts/deploy-production.js",
        "test:auth": "node scripts/test-auth.js",
        "db:init": "node scripts/init-database.js",
        "db:push": "npx prisma db push",
        "db:studio": "npx prisma studio",
        "verify": "node scripts/verify-deployment.js"
    },
