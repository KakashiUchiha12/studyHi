{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "noEmit": false,
    "moduleResolution": "node",
    "target": "es2020",
    "isolatedModules": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "strict": false
  },
  "include": [
    "server.ts",
    "next-auth.d.ts"
  ]
}